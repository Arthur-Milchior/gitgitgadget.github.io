{
    "[typescript]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 80,
        "files.trimTrailingWhitespace": true
    },
    "markdownlint.config": {
        "MD033": {
            "allowed_elements": [
                "img"
            ]
        }
    },
    "cSpell.words": [
        "Cygwin",
        "Git's",
        "Probot",
        "Schindelin",
        "amlog",
        "basedon",
        "busybox",
        "coverageprocessor",
        "dugite",
        "gitgitgadget",
        "gitster",
        "googlegroups",
        "latesttag",
        "latesttags",
        "maildir",
        "mailsplit",
        "maint",
        "mbox",
        "onelines",
        "promisify",
        "publishtoremote",
        "rebased",
        "refname",
        "repo",
        "smee",
        "smtp",
        "superset",
        "taggerdate",
        "tbdiff",
        "worktree"
    ],
    "cSpell.language": "en-US",
    "cSpell.enabledLanguageIds": [
        "git-commit",
        "html",
        "javascript",
        "json",
        "markdown",
        "plaintext",
        "typescript"
    ]
}